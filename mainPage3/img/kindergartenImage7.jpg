<script type="text/javascript">
$(function(){
	/* ===[왼쪽 슬라이드 메뉴]=== */
	//배경
	function bg(){
		$('<div class="bg-black"></div>').clone().prependTo($(parent.document).find('body'));
		$("body").css("overflow", "hidden");
		$('.bg-black').bind('click',function(){
			hideMN();
		});
	}
	//왼쪽메뉴
	$(".btn_leftMN").click(function(){
		$("#sideMN").stop().animate({"left":"0%"}, 500);
		bg();
		return false;
	});
	$(".btn_sideMN").click(function(){
		hideMN();
	});
	// 왼쪽 메뉴 닫기
	function hideMN(){
		$("#sideMN").stop().animate({"left":"-100%"}, 500);
		$('.bg-black').remove();
		$("body").css("overflow", "auto");
	}

	/* ===[상품군 팝업]=== */
	//배경
	function bg02(){
		$('.bg-black').remove();
		$('<div class="bg-black"></div>').clone().prependTo($(parent.document).find('body'));
		$('.bg-black').click(function(){
			popGoodsHide();
		});
	}
	//상품군 안내
	$(".ico_goods").click(function(){
		bg02();
		$('#pop_goods').fadeIn(300);
		return false;
	});
	//상품군 닫기
	$("#pop_goods .btn_popX").click(function(){
		$('#pop_goods').fadeOut(300);
		$('.bg-black').remove();
		return false;
	});
	//배경 클릭시 팝업 닫힘
	function popGoodsHide(){
		$('#pop_goods').fadeOut(300);
		$('.bg-black').remove();
		return false;
	}
});
//상품선택
function selProduct(type){
	if(type ==''){
		$.cookie('search_lv', type, { expires: -1, path: '/', secure: false });
	} else {
		//쿠키 7일동안 심음
		$.cookie('search_lv', type, { expires: 7, path: '/', secure: false });
	}
	//검색결과
	//psQuery로 해놓은이유 -> 작가명 검색일경우 오류가 남
	var psQuery = "jv11097561";
	var query = "";

	location.href="/search/이미지/"+psQuery+"?lv="+type+"&st=union&page=1&q="+query;
}
// New 업데이트 | 기획촬영
function goUpdate(arrParam){
	let title = arrParam['title'];
	let seq = arrParam['seq'];
	let type = arrParam['type'];

	//쿠키 7일동안 심음
	$.cookie('search_lv', '', { expires: 7, path: '/', secure: false });

	//검색결과
	var query = "UPDATE"+seq;

	// 타이틀 인코딩
	const arrReplace = ["?", "&", "!", ",", ":", "."];
	$.each(arrReplace, function(i, regex){
		title = title.replace(regex, '');
	});
	title = encodeURI(title);
	if(type == 'video_kr') {
        location.href="/search/국내동영상/"+$.trim(title)+"?q="+query+"&cl="+seq;
    } else if(type == 'video_en') {
		location.href="/search/해외동영상/"+$.trim(title)+"?q="+query+"&cl="+seq;
	} else{
        location.href="/search/이미지/"+$.trim(title)+"?q="+query+"&cl="+seq;
    }
}
</script>
