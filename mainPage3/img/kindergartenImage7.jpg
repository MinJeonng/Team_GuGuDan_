	<script>
	//보드담기 함수
	function boardInsert(val){
		//이미지seq
		image_seq = "119500317";
		//보드seq
		board_seq = val;

		if(val!=''){
			$.ajax({
				url : '/board/insertBoardImages',
				data : {
					image_seq : image_seq,
					board_seq : board_seq
				},
				dataType: 'json',
				type : 'post',
				success : function(data){
					if(data.result == true){
						if(data.stat=='success'){
							hideBg();
							//선택된 board_seq를 쿠키굽는다.
							$.cookie('select_board_seq', board_seq, { expires: 7, path: '/', secure: false });
							$('.btnBorad').hide();
							$('.btnBoradEnd').show();
							//툴팁부분
							$('#boardName').text(data.title);
							$('#tooltipDetail').text("보드에 이미지가 추가되었습니다.");
							$('#boardName').show();
							$('#tooltipBox').show();
							$('.lineSelect option:eq(0)').prop("selected",true);
							tooltipClose();
							//location.reload();
						} else if(data.stat =='exist'){
							//툴팁부분
							$('#boardName').hide();
							$('#tooltipDetail').text("보드에 동일한 이미지가 담겨있습니다.");
							$('#tooltipBox').show();
							$('.lineSelect option:eq(0)').prop("selected",true);
							tooltipClose();
							return;
						}
					} else {
						//툴팁부분
						$('#boardName').hide();
						$('#tooltipDetail').text("이미지 담기에 실패하였습니다.");
						$('#tooltipBox').show();
						$('.lineSelect option:eq(0)').prop("selected",true);
						tooltipClose();
						return;
					}
                    $('.lineSelect').hide();
				}
			});
		} else {
			alert("담으실 보드를 선택해주세요.");
			return;
		}
	}
	//보드에 담긴 이미지 빼기
	function delBoardimage(){
		//이미지seq
		image_seq = "119500317";
		//보드seq
		board_seq = $.cookie('select_board_seq');

		$.ajax({
			url : '/board/delBoardImages',
			data : {
				image_seq : image_seq,
				board_seq : board_seq
			},
			dataType: 'json',
			type : 'post',
			success : function(data){
				if(data.result=="true"){
					$('.btnBorad').show();
					$('.btnBoradEnd').hide();
					//툴팁부분
					$('#boardName').text(data.boardTitle);
					$('#tooltipDetail').text("보드에 이미지가 제거되었습니다.");
					$('#boardName').show();
					$('#tooltipBox').show();
					$('.lineSelect option:eq(0)').prop("selected",true);
					tooltipClose();
				} else {
					$('.btnBorad').hide();
					$('.btnBoradEnd').show();
				}
			}
		});
	}
	//보드 목록 다시 숨기면서 모달 숨기는 기능
	function hideBg(){
		$(".selectBorad").stop().animate({bottom:"-22"}, 400);
		$(".viewBottom").css("border-top", "1px solid #a8a8a8");
		$('.bg-black').remove();
	}
</script>
